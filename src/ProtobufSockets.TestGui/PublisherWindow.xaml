<Window x:Class="ProtobufSockets.TestGui.PublisherWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Publisher" Height="782" Width="240">
    <Grid>
        <Label Name="AddressLabel" Content="Publisher" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button Content="Start" HorizontalAlignment="Left" Margin="10,33,0,0" VerticalAlignment="Top" Width="75" Click="Publisher_Start_Button_Click"/>
        <Button Content="Stop" HorizontalAlignment="Left" Margin="90,33,0,0" VerticalAlignment="Top" Width="75" Click="Publisher_Stop_Button_Click"/>
        <Label Name="PublisherLabel" Content="RATE" HorizontalAlignment="Left" Margin="10,81,0,0" VerticalAlignment="Top"/>
        <TextBox Margin="10,103,10,10" TextWrapping="Wrap" Name="PublisherTextBox"/>
        <CheckBox Name="SendDataCheckBox" Content="Send Data" HorizontalAlignment="Left" Margin="162,10,0,0" VerticalAlignment="Top" Checked="SendDataCheckBox_OnChecked" Unchecked="SendDataCheckBox_OnUnchecked"/>
        <TextBox Name="TopicTextBox" HorizontalAlignment="Left" Height="23" Margin="46,58,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="119" TextChanged="TextBox_TextChanged"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,63,0,0" TextWrapping="Wrap" Text="Topic:" VerticalAlignment="Top"/>
    </Grid>
</Window>
